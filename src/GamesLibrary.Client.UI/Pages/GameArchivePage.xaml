<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GamesLibrary.Client.UI.Pages.GameArchivePage">
  <ContentPage.ToolbarItems>
    <ToolbarItem Name="Menu1" Order="Primary" Priority="0" />
    <ToolbarItem Name="Menu2" Order="Primary" Priority="1" />
  </ContentPage.ToolbarItems>

  <StackLayout>
    
  <Label Text="buumbuum" VerticalOptions="Center" HorizontalOptions="Center"
         Style="{DynamicResource BodyStyle}"/>

  <Label Text="buumbuum" VerticalOptions="Center" HorizontalOptions="Center"
         Style="{DynamicResource CaptionStyle}"/>

  <Label Text="buumbuum" VerticalOptions="Center" HorizontalOptions="Center"
         Style="{DynamicResource ListItemDetailTextStyle}"/>

  <Label Text="buumbuum" VerticalOptions="Center" HorizontalOptions="Center"
         Style="{DynamicResource ListItemTextStyle}"/>

  <Label Text="buumbuum" VerticalOptions="Center" HorizontalOptions="Center"
         Style="{DynamicResource SubtitleStyle}"/>
  <Label Text="buumbuum" VerticalOptions="Center" HorizontalOptions="Center"
         Style="{DynamicResource TitleStyle}"/>
  </StackLayout>

</ContentPage>